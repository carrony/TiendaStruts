<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="false"/>

    <package name="myPackage" extends="struts-default">
        <action name="index">
        	<result>/index.jsp</result>
        </action>
        <action name="crear_producto" >
            <result>/WEB-INF/jsp/registroProducto.jsp</result>
        </action>
        <action name="registrar_producto" class="com.dred.struts.tiendastruts.ProductoAction">
            <result>/WEB-INF/jsp/resumenProducto.jsp</result>
            <result name="input">/WEB-INF/jsp/registroProducto.jsp</result>
        </action>
    </package>

</struts>